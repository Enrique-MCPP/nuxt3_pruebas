{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport { useTableBean } from '@/composables/useTableBean';\nimport PedidoService from '@/service/PedidoService';\nimport { computed } from 'vue';\nexport default {\n  data() {\n    return {\n      tableBean: null\n    };\n  },\n  created() {\n    const pedidoService = new PedidoService();\n    const axiosConfig = computed(() => this.axiosConfig);\n    const {\n      editedRow,\n      save\n    } = useTableBean(pedidoService, axiosConfig);\n  },\n  setup() {\n    const doSomethingAfterSave = () => {\n      console.log('Se ha guardado la información');\n      console.log(this.tableBean.editedRow);\n    };\n    const saveWithLogic = () => {\n      // Cambiar el valor de editedRow aquí\n      this.tableBean.editedRow = {\n        id: 1,\n        estado: \"aprobado\",\n        productos: [],\n        precioTotal: 15,\n        direccionEnvio: \"pumba\",\n        metodoDePago: \"tarjeta\",\n        numSeguimiento: \"otro\",\n        fechaCreacion: new Date(),\n        observaciones: \"Primer editedRow al composable\"\n      };\n      this.tableBean.save().then(() => {\n        doSomethingAfterSave();\n      });\n    };\n    return {\n      saveWithLogic,\n      editedRow,\n      save\n    };\n  },\n  computed: {\n    ...mapGetters({\n      axiosConfig: 'axiosConfig'\n    })\n  }\n};","map":{"version":3,"names":["mapGetters","useTableBean","PedidoService","computed","data","tableBean","created","pedidoService","axiosConfig","editedRow","save","setup","doSomethingAfterSave","console","log","saveWithLogic","id","estado","productos","precioTotal","direccionEnvio","metodoDePago","numSeguimiento","fechaCreacion","Date","observaciones","then"],"sources":["C:\\Users\\emalc\\OneDrive\\Escritorio\\CRUD_keycloak_primeVue\\keycloak-workshop\\frontend-vuejs\\src\\componentes\\PedidosCRUD.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <button @click=\"saveWithLogic\">Guardar</button>\r\n  </div>\r\n</template>\r\n\r\n<script