eate(this.axiosConfig).then(response => {\r\n        this.toast.show({ severity: \"success\", summary: \"Éxito\", detail: \"Producto creado correctamente\", life: 3000 });\r\n        this.editedRow = {\r\n          id: null,\r\n          nombre: null,\r\n          descripcion: null,\r\n          precio: null\r\n        }\r\n      });\r\n    },\r\n    edit() {\r\n      this.productoService.edit(this.editedRow, this.axiosConfig).then(response => {\r\n        this.toast.show({ severity: \"success\", summary: \"Éxito\", detail: \"Producto editado correctamente\", life: 3000 });\r\n        this.editedRow = {\r\n          id: null,\r\n          nombre: null,\r\n          descripcion: null,\r\n          precio: null\r\n        }\r\n      });\r\n    },\r\n  }\r\n\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}