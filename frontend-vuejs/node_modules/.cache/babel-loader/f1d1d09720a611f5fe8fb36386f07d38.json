{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport { useTableBean } from '@/composables//useTableBean';\nimport PedidoService from '@/service/PedidoService';\nimport { computed } from 'vue';\nexport default {\n  setup() {\n    const pedidoService = new PedidoService();\n    const axiosConfig = computed(() => this.axiosConfig); // Aquí accedes a axiosConfig a través de una variable local\n    const {\n      editedRow,\n      selectedRow,\n      list,\n      isDisabled\n    } = useTableBean(pedidoService, axiosConfig);\n    const doSomethingAfterSave = () => {\n      console.log('Se ha guardado la información');\n    };\n    const saveWithLogic = () => {\n      save(editedRow.value).then(() => {\n        doSomethingAfterSave();\n      });\n    };\n    return {\n      editedRow,\n      selectedRow,\n      list,\n      isDisabled,\n      getAll,\n      updateRow,\n      remove,\n      saveWithLogic\n    };\n  },\n  computed: {\n    ...mapGetters({\n      username: 'username',\n      keycloakToken: 'keycloakToken',\n      keycloakTokenParsed: 'keycloakTokenParsed',\n      axiosConfig: 'axiosConfig',\n      roles: 'roles'\n    })\n  }\n};","map":{"version":3,"names":["mapGetters","useTableBean","PedidoService","computed","setup","pedidoService","axiosConfig","editedRow","selectedRow","list","isDisabled","doSomethingAfterSave","console","log","saveWithLogic","save","value","then","getAll","updateRow","remove","username","keycloakToken","keycloakTokenParsed","roles"],"sources":["C:\\Users\\emalc\\OneDrive\\Escritorio\\CRUD_keycloak_primeVue\\keycloak-workshop\\frontend-vuejs\\src\\componentes\\PedidosCRUD.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <button @click=\"saveWithLogic\">Guardar</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nimport { useTableBean } from '@/composables//useTableBean';\r\nimport PedidoService from '@/service/PedidoService';\r\nimport { computed } from 'vue';\r\n\r\nexport default {\