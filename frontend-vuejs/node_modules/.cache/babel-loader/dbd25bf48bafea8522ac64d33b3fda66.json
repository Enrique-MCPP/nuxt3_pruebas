{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport { useTableBean } from '@/composables/useTableBean';\nimport PedidoService from '@/service/PedidoService';\nimport { computed } from 'vue';\nexport default {\n  setup() {\n    const pedidoService = new PedidoService();\n    const axiosConfig = computed(() => this.axiosConfig);\n    const {\n      state,\n      save,\n      setEditedRow\n    } = useTableBean(pedidoService, axiosConfig);\n    const {\n      stateUno,\n      updateMyObject\n    } = useTableBean();\n    const doSomethingAfterSave = () => {\n      console.log('Se ha guardado la información');\n      console.log('Dentro del doSomethingAfterSave');\n      console.log(state.editedRow);\n    };\n    const saveWithLogic = () => {\n      console.log(state.editedRow);\n      setEditedRow({\n        id: 2,\n        estado: \"suspenso\",\n        productos: [],\n        precioTotal: 15,\n        direccionEnvio: \"otro lado\",\n        metodoDePago: \"paypal\",\n        numSeguimiento: \"otro\",\n        fechaCreacion: new Date(),\n        observaciones: \"Primer editedRow al composable\"\n      });\n      console.log(state.editedRow);\n      save().then(() => {\n        doSomethingAfterSave();\n      });\n    };\n    return {\n      saveWithLogic,\n      state,\n      save,\n      setEditedRow,\n      stateUno,\n      updateMyObject\n    };\n  },\n  computed: {\n    ...mapGetters({\n      axiosConfig: 'axiosConfig'\n    })\n  }\n};","map":{"version":3,"names":["mapGetters","useTableBean","PedidoService","computed","setup","pedidoService","axiosConfig","state","save","setEditedRow","stateUno","updateMyObject","doSomethingAfterSave","console","log","editedRow","saveWithLogic","id","estado","productos","precioTotal","direccionEnvio","metodoDePago","numSeguimiento","fechaCreacion","Date","observaciones","then"],"sources":["C:\\Users\\emalc\\OneDrive\\Escritorio\\CRUD_keycloak_primeVue\\keycloak-workshop\\frontend-vuejs\\src\\componentes\\PedidosCRUD.vue"],