{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport { tableBean } from '@/composables/tableBean';\nimport PedidoService from '@/service/PedidoService';\nimport { computed } from 'vue';\nimport { watchEffect } from 'vue';\nexport default {\n  setup() {\n    const pedidoService = new PedidoService();\n    const {\n      state,\n      list,\n      save,\n      setEditedRow\n    } = tableBean(pedidoService);\n    const axiosConfig = computed(() => this.$store.getters.axiosConfig);\n    const doSomethingAfterSave = () => {\n      console.log('Se ha guardado la informaci贸n');\n      console.log('Dentro del doSomethingAfterSave');\n      console.log(state.editedRow);\n    };\n    const savePedido = () => {\n      save().then(() => {\n        doSomethingAfterSave();\n      });\n    };\n\n    // llama a la funci贸n list del composable cuando se monta el componente y cuando se actualiza axiosConfig\n    watchEffect(async () => {\n      if (axiosConfig.value) {\n        try {\n          const response = await pedidoService.getAll(axiosConfig.value);\n          state.list = response.data;\n        } catch (error) {\n          console.log(error);\n        }\n      }\n    });\n    return {\n      savePedido,\n      state,\n      setEditedRow,\n      list\n    };\n  },\n  created() {\n    console.log(\"created\");\n    this.initFilters();\n    this.getAll();\n  },\n  computed: {\n    ...mapGetters({\n      axiosConfig: 'axiosConfig'\n    })\n  }\n};","map":{"version":3,"names":["mapGetters","tableBean","PedidoService","computed","watchEffect","setup","pedidoService","state","list","save","setEditedRow","axiosConfig","$store","getters","doSomethingAfterSave","console","log","editedRow","savePedido","then","value","response","getAll","data","error","created","initFilters"],"sources":["C:\\Users\\emalc\\OneDrive\\Escritorio\\CRUD_keycloak_primeVue\\keycloak-workshop\\frontend-vuejs\\src\\componentes\\PedidosCRUD.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <button 