{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport { tableBean } from '@/composables/tableBean';\nimport PedidoService from '@/service/PedidoService';\nimport { computed } from 'vue';\nimport { watchEffect } from 'vue';\nexport default {\n  setup() {\n    const pedidoService = new PedidoService();\n    const axiosConfig = computed(() => this.$store.getters.axiosConfig);\n    const {\n      state,\n      list,\n      save,\n      setEditedRow\n    } = tableBean(pedidoService, axiosConfig);\n    const doSomethingAfterSave = () => {\n      console.log('Se ha guardado la información');\n      console.log('Dentro del doSomethingAfterSave');\n      console.log(state.editedRow);\n    };\n    const savePedido = () => {\n      save().then(() => {\n        doSomethingAfterSave();\n      });\n    };\n    watchEffect(async () => {\n      if (axiosConfig.value) {\n        try {\n          const response = await pedidoService.getAll(axiosConfig.value);\n          state.list = response.data;\n        } catch (error) {\n          console.log(error);\n        }\n      }\n    });\n    return {\n      savePedido,\n      state,\n      setEditedRow,\n      list\n    };\n  },\n  computed: {\n    ...mapGetters({\n      axiosConfig: 'axiosConfig'\n    })\n  }\n};","map":{"version":3,"names":["mapGetters","tableBean","PedidoService","computed","watchEffect","setup","pedidoService","axiosConfig","$store","getters","state","list","save","setEditedRow","doSomethingAfterSave","console","log","editedRow","savePedido","then","value","response","getAll","data","error"],"sources":["C:\\Users\\emalc\\OneDrive\\Escritorio\\CRUD_keycloak_primeVue\\keycloak-workshop\\frontend-vuejs\\src\\componentes\\PedidosCRUD.vue"],"sourcesContent":["<template>\r\n  <div>\r\n\r\n    <button @click=\"updateMyObject\">Actualizar objeto</button>\r\n    <button @click=\"saveWithLogic\">Actualizar editedRow</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nimport { tableBean } from '@/composables/tableBean';\r\nimport PedidoService from '@/service/PedidoService';\r\nimport { computed } from 'vue';\r\nimport { watchEffect } from 'vue';\r\n\r\nexport default {\r\n  setup() {\r\n    const pedidoService = new PedidoService();\r\n    const axiosConfig = computed(() => this.$store.getters.axiosConfig);\r\n\r\n\r\n    const { state, list, save, setEditedRow } = tableBean(pedidoService, axiosConfig);\r\n\r\n    const doSomethingAfterSave = () => {\r\n      console.log('Se ha guardado la información');\r\n      console.log('Dentro del doSomethingAfterSave')\r\n      console.log(state.editedRow);\r\n    };\r\n\r\n    const savePedido = () => {\r\n      save().then(() => {\r\n        doSomethingAfterSave();\r\n      });\r\n    };\r\n\r\n\r\n\r\n    watchEffect(async () => {\r\n      if (axiosConfig.value) {\r\n        try {\r\n          const response = await pedidoService.getAll(axiosConfig.value);\r\n          state.list = response.data;\r\n        } catch (error) {\r\n          console.log(error);\r\n        }\r\n      }\r\n    });\r\n\r\n    return {\r\n      savePedido,\r\n      state,\r\n      setEditedRow,\r\n      list\r\n\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters({\r\n      axiosConfig: 'axiosConfig',\r\n    }),\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AASA,SAASA,UAAS,QAAS,MAAM;AACjC,SAASC,SAAQ,QAAS,yBAAyB;AACnD,OAAOC,aAAY,MAAO,yBAAyB;AACnD,SAASC,QAAO,QAAS,KAAK;AAC9B,SAASC,WAAU,QAAS,KAAK;AAEjC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,aAAY,GAAI,IAAIJ,aAAa,EAAE;IACzC,MAAMK,WAAU,GAAIJ,QAAQ,CAAC,MAAM,IAAI,CAACK,MAAM,CAACC,OAAO,CAACF,WAAW,CAAC;IAGnE,MAAM;MAAEG,KAAK;MAAEC,IAAI;MAAEC,IAAI;MAAEC;IAAa,IAAIZ,SAAS,CAACK,aAAa,EAAEC,WAAW,CAAC;IAEjF,MAAMO,oBAAmB,GAAIA,CAAA,KAAM;MACjCC,OAAO,CAACC,GAAG,CAAC,+BAA+B,CAAC;MAC5CD,OAAO,CAACC,GAAG,CAAC,iCAAiC;MAC7CD,OAAO,CAACC,GAAG,CAACN,KAAK,CAACO,SAAS,CAAC;IAC9B,CAAC;IAED,MAAMC,UAAS,GAAIA,CAAA,KAAM;MACvBN,IAAI,EAAE,CAACO,IAAI,CAAC,MAAM;QAChBL,oBAAoB,EAAE;MACxB,CAAC,CAAC;IACJ,CAAC;IAID