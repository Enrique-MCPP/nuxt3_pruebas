{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport { tableBean } from '@/composables/tableBean';\nimport PedidoService from '@/service/PedidoService';\nimport { computed } from 'vue';\nimport { watchEffect } from 'vue';\nexport default {\n  setup() {\n    const pedidoService = new PedidoService();\n    const axiosConfig = computed(() => this.axiosConfig);\n    const {\n      state,\n      list,\n      save,\n      setEditedRow\n    } = tableBean(pedidoService, axiosConfig);\n    const doSomethingAfterSave = () => {\n      console.log('Se ha guardado la informaci贸n');\n      console.log('Dentro del doSomethingAfterSave');\n      console.log(state.editedRow);\n    };\n    const savePedido = () => {\n      save().then(() => {\n        doSomethingAfterSave();\n      });\n    };\n\n    // llama a la funci贸n list del composable cuando se monta el componente y cuando se actualiza axiosConfig\n    watchEffect(async () => {\n      try {\n        const response = await pedidoService.getAll(axiosConfig);\n        state.list = response.data;\n      } catch (error) {\n        console.log(error);\n      }\n    });\n    return {\n      savePedido,\n      state,\n      setEditedRow,\n      list\n    };\n  },\n  computed: {\n    ...mapGetters({\n      axiosConfig: 'axiosConfig'\n    })\n  }\n};","map":{"version":3,"names":["mapGetters","tableBean","PedidoService","computed","watchEffect","setup","pedidoService","axiosConfig","state","list","save","setEditedRow","doSomethingAfterSave","console","log","editedRow","savePedido","then","response","getAll","data","error"],"sources":["C:\\Users\\emalc\\OneDrive\\Escritorio\\CRUD_keycloak_primeVue\\keycloak-workshop\\frontend-vuejs\\src\\componentes\\PedidosCRUD.vue"],"sourcesContent":["<template>\r\n  <div>\r\n\r\n    <button @click=\"updateMyObject\">Actualizar objeto</button>\r\n    <button @click=\"saveWithLogic\">Actualizar editedRow</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r