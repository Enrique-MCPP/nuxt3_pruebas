{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport { tableBean } from '@/composables/tableBean';\nimport PedidoService from '@/service/PedidoService';\nimport { computed, useEffect } from 'vue';\nimport { watchEffect } from 'vue';\nexport default {\n  setup() {\n    const pedidoService = new PedidoService();\n    const axiosConfig = computed(() => this.axiosConfig);\n    const {\n      state,\n      list,\n      save,\n      setEditedRow\n    } = tableBean(pedidoService, axiosConfig);\n    const doSomethingAfterSave = () => {\n      console.log('Se ha guardado la información');\n      console.log('Dentro del doSomethingAfterSave');\n      console.log(state.editedRow);\n    };\n    const savePedido = () => {\n      save().then(() => {\n        doSomethingAfterSave();\n      });\n    };\n    useEffect(() => {\n      // Verificar si el usuario está autenticado y obtener el token de acceso si es necesario\n      // Actualizar axiosConfig con el token de acceso\n      // ...\n    }, []);\n\n    // llama a la función list del composable cuando se monta el componente y cuando se actualiza axiosConfig\n    watchEffect(() => {\n      list();\n    });\n    return {\n      savePedido,\n      state,\n      setEditedRow,\n      list\n    };\n  },\n  computed: {\n    ...mapGetters({\n      axiosConfig: 'axiosConfig'\n    })\n  }\n};","map":{"version":3,"names":["mapGetters","tableBean","PedidoService","computed","useEffect","watchEffect","setup","pedidoService","axiosConfig","state","list","save","setEditedRow","doSomethingAfterSave","console","log","editedRow","savePedido","then"],"sources":["C:\\Users\\emalc\\OneDrive\\Escritorio\\CRUD_keycloak_primeVue\\keycloak-workshop\\frontend-vuejs\\src\\componentes\\PedidosCRUD.vue"],"sourcesContent":["<template>\r\n  <div>\r\n\r\n    <button @click=\"updateMyObject\">Actualizar objeto</button>\r\n    <button @click=\"saveWithLogic\">Actualizar editedRow</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nimport { tableBean } from '@/composables/tableBean';\r\nimport PedidoService from '@/service/PedidoService';\r\nimport { computed, useEffect } from 'vue';\r\nimport { watchEffect } from 'vue';\r\n\r\nexport default {\r\n  setup() {\r\n    const pedidoService = new PedidoService();\r\n    const axiosConfig = computed(() => this.axiosConfig);\r\n\r\n    const { state, list, save, setEditedRow } = tableBean(pedidoService, axiosConfig);\r\n\r\n    const doSomethingAfterSave = () => {\r\n      console.log('Se ha guardado la información');\r\n      console.log('Dentro del doSomethingAfterSave')\r\n      console.log(state.editedRow);\r\n    };\r\n\r\n    const savePedido = () => {\r\n      save().then(() => {\r\n        doSomethingAfterSave();\r\n      });\r\n    };\r\n\r\n    useEffect(() => {\r\n      // Verificar si el usuario está autenticado y obtener el token de acceso si es necesario\r\n      // Actualizar axiosConfig con el token de acceso\r\n      // ...\r\n    }, []);\r\n\r\n    // llama a la función list del composable cuando se monta el componente y cuando se actualiza axiosConfig\r\n    watchEffect(() => {\r\n      list();\r\n    });\r\n\r\n    return {\r\n      savePedido,\r\n      state,\r\n      setEditedRow,\r\n      list\r\n\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters({\r\n      axiosConfig: 'axiosConfig',\r\n    }),\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AASA,SAASA,UAAS,QAAS,MAAM;AACjC,SAASC,SAAQ,QAAS,yBAAyB;AACnD,OAAOC,aAAY,MAAO,yBAAyB;AACnD,SAASC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AACzC,SAASC,WAAU,QAAS,KAAK;AAEjC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,aAAY,GAAI,IAAIL,aAAa,EAAE;IACzC,MAAMM,WAAU,GAAIL,QAAQ,CAAC,MAAM,IAAI,CAACK,WAAW,CAAC;IAEpD,MAAM;MAAEC,KAAK;MAAEC,IAAI;MAAEC,IAAI;MAAEC;IAAa,IAAIX,SAAS,CAACM,aAAa,EAAEC,WAAW,CAAC;IAEjF,MAAMK,oBAAmB,GAAIA,CAAA,KAAM;MACjCC,OAAO,CAACC,GAAG,CAAC,+BAA+B,CAAC;MAC5CD,OAAO,CAACC,GAAG,CAAC,iCAAiC;MAC7CD,OAAO,CAACC,GAAG,CAACN,KAAK,CAACO,SAAS,CAAC;IAC9B,CAAC;IAED,MAAMC,UAAS,GAAIA,CAAA,KAA