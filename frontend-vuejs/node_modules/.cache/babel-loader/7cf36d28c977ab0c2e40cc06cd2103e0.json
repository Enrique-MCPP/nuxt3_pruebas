{"ast":null,"code":"import { FilterMatchMode } from 'primevue/api';\nimport { mapGetters } from 'vuex';\nimport axios from \"axios\";\nexport default {\n  props: {\n    columns: {\n      type: Array,\n      required: true\n    },\n    data: {\n      type: Array,\n      required: true\n    },\n    header: {\n      type: String,\n      default: \"Lista de elementos\"\n    },\n    editedRow: {\n      type: Object,\n      default: null\n    },\n    crudInputs: {\n      type: Object,\n      default: function () {\n        return {};\n      }\n    }\n  },\n\n  data() {\n    // Define una caché para almacenar las opciones obtenidas del backend\n    const optionsCache = {};\n    return {\n      filters: {},\n      selectedRow: null,\n      localEditedRow: Object.assign({}, this.editedRow),\n      optionsData: []\n    };\n  },\n\n  watch: {\n    editedRow: {\n      handler: function (newVal) {\n        this.localEditedRow = Object.assign({}, newVal);\n      },\n      deep: true\n    }\n  },\n  computed: {\n    globalFilterFields() {\n      return ['global', ...this.columns.map(column => column.field)];\n    },\n\n    currentPageReportTemplate() {\n      return `Mostrando {first} a {last} de ${this.data.length} pedidos`;\n    },\n\n    currentSelection() {\n      return this.selectedRow[0] || {};\n    },\n\n    ...mapGetters({\n      username: 'username',\n      keycloakToken: 'keycloakToken',\n      keycloakTokenParsed: 'keycloakTokenParsed',\n      axiosConfig: 'axiosConfig',\n      roles: 'roles'\n    })\n  },\n\n  async created() {\n    console.log(\"created\");\n    this.initFilters();\n  },\n\n  mounted() {\n    // Recorre los inputs y si existe pathList, realiza una solicitud GET para obtener las opciones del selectOneMenu\n    this.crudInputs.forEach(async input => {\n      if (input.component.includes('select') && input.pathList) {\n        // Si el pathList se encuentra en la caché, utiliza la lista de opciones existente\n        if (optionsCache[input.pathList]) {\n          input.options = optionsCache[input.pathList];\n        } else {\n          // Si el pathList no se encuentra en la caché, realiza una solicitud al backend y almacena la lista de opciones en la caché\n          const response = await axios.get(input.pathList, this.axiosConfig);\n          const parsedData = response.data;\n          const optionsList = parsedData.map(item => ({\n            label: item.nombre,\n            value: item\n          }));\n          optionsCache[input.pathList] = optionsList;\n          input.options = optionsList;\n        }\n      }\n    });\n  },\n\n  methods: {\n    onRowSelect(event) {\n      this.localEditedRow = Object.assign({}, event.data);\n    },\n\n    initFilters() {\n      const filters = {\n        'global': {\n          value: null,\n          matchMode: FilterMatchMode.CONTAINS\n        }\n      };\n      this.columns.forEach(column => {\n        filters[column.field] = {\n          value: null,\n          matchMode: FilterMatchMode.CONTAINS\n        };\n      });\n      this.filters = filters;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyDA,SAASA,eAAT,QAAgC,cAAhC;AACA,SAASC,UAAT,QAA2B,MAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;AAEXC,OAAK,EAAE;AACHC,WAAO,EAAE;AACLC,UAAI,EAAEC,KADD;AAELC,cAAQ,EAAE;AAFL,KADN;AAKHC,QAAI,EAAE;AACFH,UAAI,EAAEC,KADJ;AAEFC,cAAQ,EAAE;AAFR,KALH;AASHE,UAAM,EAAE;AACJJ,UAAI,EAAEK,MADF;AAEJC,aAAO,EAAE;AAFL,KATL;AAaHC,aAAS,EAAE;AACPP,UAAI,EAAEQ,MADC;AAEPF,aAAO,EAAE;AAFF,KAbR;AAiBHG,cAAU,EAAE;AACRT,UAAI,EAAEQ,MADE;AAERF,aAAO,EAAE,YAAY;AACjB,eAAO,EAAP;AACJ;AAJQ;AAjBT,GAFI;;AA6BXH,MAAI,GAAG;AACH;AACA,UAAMO,YAAW,GAAI,EAArB;AAEA,WAAO;AACHC,aAAO,EAAE,EADN;AAEHC,iBAAW,EAAE,IAFV;AAGHC,oBAAc,EAAEL,MAAM,CAACM,MAAP,CAAc,EAAd,EAAkB,KAAKP,SAAvB,CAHb;AAIHQ,iBAAW,EAAE;AAJV,KAAP;AAQH,GAzCU;;AA0CXC,OAAK,EAAE;AACHT,aAAS,EAAE;AACPU,aAAO,EAAE,UAAUC,MAAV,EAAkB;AACvB,aAAKL,cAAL,GAAsBL,MAAM,CAACM,MAAP,CAAc,EAAd,EAAkBI,MAAlB,CAAtB;AACH,OAHM;AAIPC,UAAI,EAAE;AAJC;AADR,GA1CI;AAoDXC,UAAQ,EAAE;AACNC,sBAAkB,GAAG