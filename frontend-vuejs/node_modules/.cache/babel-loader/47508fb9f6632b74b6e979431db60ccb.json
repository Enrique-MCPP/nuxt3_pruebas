{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport UseGenericCRUD from './componentes/UseGenericCRUD.vue';\nimport PedidoDatatable from './componentes/PedidoDatatable.vue';\nexport default {\n  name: 'App',\n  components: {\n    UseGenericCRUD,\n    PedidoDatatable\n  },\n\n  data() {\n    return {\n      backendToken: null\n    };\n  },\n\n  computed: { ...mapGetters({\n      username: 'username',\n      keycloakToken: 'keycloakToken',\n      keycloakTokenParsed: 'keycloakTokenParsed',\n      roles: 'roles'\n    })\n  },\n  methods: {\n    logout() {\n      this.emitter.emit('keycloak-logout');\n    },\n\n    getBackendTokenInfo() {\n      const config = {\n        headers: {\n          Authorization: `Bearer ${this.keycloakToken}`\n        }\n      };\n      this.axios.get(\"api/product/\", config).then(response => {\n        this.backendToken = response.data;\n      });\n    },\n\n    filteredRoles() {\n      return this.roles.filter(role => !role.includes(\"offline_access\") && !role.includes(\"uma_authorization\") && !role.includes(\"default-roles-spring-boot-microservices-realm\"));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA,SAASA,UAAT,QAA2B,MAA3B;AAGA,OAAOC,cAAP,MAA2B,kCAA3B;AACA,OAAOC,eAAP,MAA4B,mCAA5B;AAIA,eAAe;AACbC,MAAI,EAAE,KADO;AAEbC,YAAU,EAAE;AAGVH,kBAHU;AAIVC;AAJU,GAFC;;AAWbG,MAAI,GAAG;AACL,WAAO;AACLC,kBAAY,EAAE;AADT,KAAP;AAGD,GAfY;;AAgBbC,UAAQ,EAAE,EACR,GAAGP,UAAU,CAAC;AACZQ,cAAQ,EAAE,UADE;AAEZC,mBAAa,EAAE,eAFH;AAGZC,yBAAmB,EAAE,qBAHT;AAIZC,WAAI,EAAI;AAJI,KAAD;AADL,GAhBG;AAwBbC,SAAO,EAAE;AACPC,UAAM,GAAG;AACP,WAAKC,OAAL,CAAaC,IAAb,CAAkB,iBAAlB;AACD,KAHM;;AAIPC,uBAAmB,GAAG;AACpB,YAAMC,MAAK,GAAI;AACbC,eAAO,EAAE;AACPC,uBAAa,EAAG,UAAS,KAAKV,aAAc;AADrC;AADI,OAAf;AAKA,WAAKW,KAAL,CAAWC,GAAX,CAAe,cAAf,EAA+BJ,MAA/B,EAAuCK,IAAvC,CAA6CC,QAAD,IAAc;AACxD,aAAKjB,YAAL,GAAoBiB,QAAQ,CAAClB,IAA7B;AACD,OAFD;AAGD,KAbM;;AAcPmB,iBAAa,GAAE;AACb,aAAO,KAAKb,KAAL,CAAWc,MAAX,CAAkBC,IAAG,IAAI,CAACA,IAAI,CAACC,QAAL,CAAc,gBAAd,CAAD,IAAmC,CAACD,IAAI,CAACC,Q