{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport { tableBean } from '@/composables/tableBean';\nimport PedidoService from '@/service/PedidoService';\nimport { computed } from 'vue';\nimport { watchEffect } from 'vue';\nexport default {\n  setup() {\n    const pedidoService = new PedidoService();\n    const {\n      state,\n      list,\n      save,\n      setEditedRow\n    } = tableBean(pedidoService);\n    const axiosConfig = computed(() => axiosConfig.value);\n    const doSomethingAfterSave = () => {\n      console.log('Se ha guardado la información');\n      console.log('Dentro del doSomethingAfterSave');\n      console.log(state.editedRow);\n    };\n    const savePedido = () => {\n      save().then(() => {\n        doSomethingAfterSave();\n      });\n    };\n    watchEffect(async () => {\n      console.log('watchEffect');\n      try {\n        const response = await pedidoService.getAll(this.axiosConfig);\n        state.list = response.data;\n      } catch (error) {\n        console.log(error);\n      }\n    });\n    return {\n      savePedido,\n      state,\n      setEditedRow,\n      list\n    };\n  },\n  computed: {\n    ...mapGetters({\n      axiosConfig: 'axiosConfig'\n    })\n  }\n};","map":{"version":3,"names":["mapGetters","tableBean","PedidoService","computed","watchEffect","setup","pedidoService","state","list","save","setEditedRow","axiosConfig","value","doSomethingAfterSave","console","log","editedRow","savePedido","then","response","getAll","data","error"],"sources":["C:\\Users\\emalc\\OneDrive\\Escritorio\\CRUD_keycloak_primeVue\\keycloak-workshop\\frontend-vuejs\\src\\componentes\\PedidosCRUD.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <button @click=\"updateMyObject\">Actualizar objeto</button>\r\n    <button @click=\"saveWithLogic\">Actualizar editedRow</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nimport { tableBean } from '@/composables/tableBean';\r\nimport PedidoService from '@/service/PedidoService';\r\nimport { computed } from 'vue';\r\nimport { watchEffect } from 'vue';\r\n\r\nexport default {\r\n  setup() {\r\n    const pedidoService = new PedidoService();\r\n\r\n    const { state, list, save, setEditedRow } = tableBean(pedidoService);\r\n\r\n    const axiosConfig = computed(() => axiosConfig.value);\r\n\r\n\r\n    const doSomethingAfterSave = () => {\r\n      console.log('Se ha guardado la información');\r\n      console.log('Dentro del doSomethingAfterSave')\r\n      console.log(state.editedRow);\r\n    };\r\n\r\n    const savePedido = () => {\r\n      save().then(() => {\r\n        doSomethingAfterSave();\r\n      });\r\n    };\r\n\r\n   \r\n    watchEffect(async () => {\r\n      console.log('watchEffect')\r\n      try {\r\n        const response = await pedidoService.getAll(this.axiosConfig);\r\n        state.list = response.data;\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n\r\n    });\r\n\r\n    return {\r\n      savePedido,\r\n      state,\r\n      setEditedRow,\r\n      list\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters({\r\n      axiosConfig: 'axiosConfig',\r\n    }),\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAQA,SAASA,UAAS,QAAS,MAAM;AACjC,SAASC,SAAQ,QAAS,yBAAyB;AACnD,OAAOC,aAAY,MAAO,yBAAyB;AACnD,SAASC,QAAO,QAAS,KAAK;AAC9B,SAASC,WAAU,QAAS,KAAK;AAEjC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,aAAY,GAAI,IAAIJ,aAAa,EAAE;IAEzC,MAAM;MAAEK,KAAK;MAAEC,IAAI;MAAEC,IAAI;MAAEC;IAAa,IAAIT,SAAS,CAACK,aAAa,CAAC;IAEpE,MAAMK,WAAU,GAAIR,QAAQ,CAAC,MAAMQ,WAAW,CAACC,KAAK,CAAC;IAGrD,MAAMC,oBAAmB,GAAIA,CAAA,KAAM;MACjCC,OAAO,CAACC,GAAG,CAAC,+BAA+B,CAAC;MAC5CD,OAAO,CAACC,GAAG,CAAC,iCAAiC;MAC7CD,OAAO,CAACC,GAAG,CAACR,KAAK,CAACS,SAAS,CAAC;IAC9B,CAAC;IAED,MAAMC,UAAS,GAAIA,CAAA,KAAM;MACvBR,IAAI,EAAE,CAACS,IAAI,CAAC,MAAM;QAChBL,oBAAoB,EAAE;MACxB,CAAC,CAAC;IACJ,CAAC;IAGDT,WAAW,CAAC,YAAY;MACtBU,OAAO,CAACC,GAAG,CAAC,aAAa;MACzB,IAAI;QACF,MAAMI,QAAO,GAAI,MAAMb,aAAa,CAACc,MAAM,CAAC,IAAI,CAACT,WAAW,CAAC;QAC7DJ,KAAK,CAACC