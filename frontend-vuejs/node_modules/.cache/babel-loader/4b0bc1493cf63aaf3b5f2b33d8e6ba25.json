{"ast":null,"code":"import { createStore } from 'vuex';\nimport { computed, provide, inject } from 'vue';\nimport PedidoService from '@/service/PedidoService';\nimport { tableBean } from '@/composables/tableBean';\nexport default {\n  setup() {\n    const pedidoService = new PedidoService();\n    const store = inject('store');\n    const axiosConfig = computed(() => store.getters.axiosConfig);\n    const {\n      state,\n      list,\n      save,\n      setEditedRow\n    } = tableBean(pedidoService, axiosConfig);\n    const doSomethingAfterSave = () => {\n      console.log('Se ha guardado la información');\n      console.log('Dentro del doSomethingAfterSave');\n      console.log(state.editedRow);\n    };\n    const savePedido = () => {\n      save().then(() => {\n        doSomethingAfterSave();\n      });\n    };\n    watchEffect(async () => {\n      console.log('watchEffect');\n      try {\n        const response = await pedidoService.getAll(axiosConfig.value);\n        state.list = response.data;\n      } catch (error) {\n        console.log(error);\n      }\n    });\n    return {\n      savePedido,\n      state,\n      setEditedRow,\n      list\n    };\n  }\n};","map":{"version":3,"names":["createStore","computed","provide","inject","PedidoService","tableBean","setup","pedidoService","store","axiosConfig","getters","state","list","save","setEditedRow","doSomethingAfterSave","console","log","editedRow","savePedido","then","watchEffect","response","getAll","value","data","error"],"sources":["C:\\Users\\emalc\\OneDrive\\Escritorio\\CRUD_keycloak_primeVue\\keycloak-workshop\\frontend-vuejs\\src\\componentes\\PedidosCRUD.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <button @click=\"updateMyObject\">Actualizar objeto</button>\r\n    <button @click=\"saveWithLogic\">Actualizar editedRow</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport { createStore } from 'vuex';\r\nimport { computed, provide, inject } from 'vue';\r\nimport PedidoService from '@/service/PedidoService';\r\nimport { tableBean } from '@/composables/tableBean';\r\n\r\nexport default {\r\n  setup() {\r\n    const pedidoService = new PedidoService();\r\n    const store = inject('store');\r\n    const axiosConfig = computed(() => store.getters.axiosConfig);\r\n\r\n    const { state, list, save, setEditedRow } = tableBean(pedidoService, axiosConfig);\r\n\r\n    const doSomethingAfterSave = () => {\r\n      console.log('Se ha guardado la información');\r\n      console.log('Dentro del doSomethingAfterSave')\r\n      console.log(state.editedRow);\r\n    };\r\n\r\n    const savePedido = () => {\r\n      save().then(() => {\r\n        doSomethingAfterSave();\r\n      });\r\n    };\r\n\r\n    watchEffect(async () => {\r\n      console.log('watchEffect')\r\n      try {\r\n        const response = await pedidoService.getAll(axiosConfig.value);\r\n        state.list = response.data;\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n\r\n    });\r\n\r\n    return {\r\n      savePedido,\r\n      state,\r\n      setEditedRow,\r\n      list,\r\n    };\r\n  },\r\n};\r\n\r\n</script>\r\n"],"mappings":"AASA,SAASA,WAAU,QAAS,MAAM;AAClC,SAASC,QAAQ,EAAEC,OAAO,EAAEC,MAAK,QAAS,KAAK;AAC/C,OAAOC,aAAY,MAAO,yBAAyB;AACnD,SAASC,SAAQ,QAAS,yBAAyB;AAEnD,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,aAAY,GAAI,IAAIH,aAAa,EAAE;IACzC,MAAMI,KAAI,GAAIL,MAAM,CAAC,OAAO,CAAC;IAC7B,MAAMM,WAAU,GAAIR,QAAQ,CAAC,MAAMO,KAAK,CAACE,OAAO,CAACD,WAAW,CAAC;IAE7D,MAAM;MAAEE,KAAK;MAAEC,IAAI;MAAEC,IAAI;MAAEC;IAAa,IAAIT,SAAS,CAACE,aAAa,EAAEE,WAAW,CAAC;IAEjF,MAAMM,oBAAmB,GAAIA,CAAA,KAAM;MACjCC,OAAO,CAACC,GAAG,CAAC,+BAA+B,CAAC;MAC5CD,OAAO,CAACC,GAAG,CAAC,iCAAiC;MAC7CD,OAAO,CAACC,GAAG,CAACN,KAAK,CAACO,SAAS,CAAC;IAC9B,CAAC;IAED,MAAMC,UAAS,GAAIA,CAAA,KAAM;MACvBN,IAAI,EAAE,CAACO,IAAI,CAAC,MAAM;QAChBL,oBAAoB,EAAE;MACxB,CAAC,CAAC;IACJ,CAAC;IAEDM,WAAW,CAAC,YAAY;MACtBL,OAAO,CAACC,GAAG,CAAC,aAAa;MACzB,IAAI;QACF,MAAMK,QAAO,GAAI,MAAMf,aAAa,CAACgB,MAAM,CAACd,WAAW,CAACe,KAAK,CAAC;QAC9Db,KAAK,CAACC,IAAG,