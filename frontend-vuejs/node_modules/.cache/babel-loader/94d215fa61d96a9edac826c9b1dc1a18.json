{"ast":null,"code":"import { FilterMatchMode } from 'primevue/api';\nimport { mapGetters } from 'vuex';\nimport axios from \"axios\";\nexport default {\n  props: {\n    columns: {\n      type: Array,\n      required: true\n    },\n    data: {\n      type: Array,\n      required: true\n    },\n    header: {\n      type: String,\n      default: \"Lista de elementos\"\n    },\n    editedRow: {\n      type: Object,\n      default: null\n    },\n    crudInputs: {\n      type: Object,\n      default: function () {\n        return {};\n      }\n    },\n    pathOptionsData: {\n      type: String\n    }\n  },\n\n  data() {\n    return {\n      filters: {},\n      selectedRow: null,\n      localEditedRow: Object.assign({}, this.editedRow),\n      optionsData: [],\n      pathOptions: Object.assign({}, this.pathOptionsData)\n    };\n  },\n\n  watch: {\n    editedRow: {\n      handler: function (newVal) {\n        this.localEditedRow = Object.assign({}, newVal);\n      },\n      deep: true\n    }\n  },\n  computed: {\n    globalFilterFields() {\n      return ['global', ...this.columns.map(column => column.field)];\n    },\n\n    currentPageReportTemplate() {\n      return `Mostrando {first} a {last} de ${this.data.length} pedidos`;\n    },\n\n    currentSelection() {\n      return this.selectedRow[0] || {};\n    },\n\n    ...mapGetters({\n      username: 'username',\n      keycloakToken: 'keycloakToken',\n      keycloakTokenParsed: 'keycloakTokenParsed',\n      axiosConfig: 'axiosConfig',\n      roles: 'roles'\n    })\n  },\n\n  created() {\n    console.log(\"created hijo\");\n    this.initFilters();\n    const response = axios.get(this.pathOptions, this.axiosConfig);\n    const parsedData = response.data;\n    this.optionsData = parsedData.map(item => ({\n      label: item.nombre,\n      value: item.id\n    }));\n    console.log(\"created hijo 2\");\n  },\n\n  methods: {\n    onRowSelect(event) {\n      this.localEditedRow = Object.assign({}, event.data);\n    },\n\n    initFilters() {\n      const filters = {\n        'global': {\n          value: null,\n          matchMode: FilterMatchMode.CONTAINS\n        }\n      };\n      this.columns.forEach(column => {\n        filters[column.field] = {\n          value: null,\n          matchMode: FilterMatchMode.CONTAINS\n        };\n      });\n      this.filters = filters;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwDA,SAASA,eAAT,QAAgC,cAAhC;AACA,SAASC,UAAT,QAA2B,MAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;AAEXC,OAAK,EAAE;AACHC,WAAO,EAAE;AACLC,UAAI,EAAEC,KADD;AAELC,cAAQ,EAAE;AAFL,KADN;AAKHC,QAAI,EAAE;AACFH,UAAI,EAAEC,KADJ;AAEFC,cAAQ,EAAE;AAFR,KALH;AASHE,UAAM,EAAE;AACJJ,UAAI,EAAEK,MADF;AAEJC,aAAO,EAAE;AAFL,KATL;AAaHC,aAAS,EAAE;AACPP,UAAI,EAAEQ,MADC;AAEPF,aAAO,EAAE;AAFF,KAbR;AAiBHG,cAAU,EAAE;AACRT,UAAI,EAAEQ,MADE;AAERF,aAAO,EAAE,YAAY;AACjB,eAAO,EAAP;AACJ;AAJQ,KAjBT;AAuBHI,mBAAe,EAAE;AACbV,UAAI,EAAEK;AADO;AAvBd,GAFI;;AA+BXF,MAAI,GAAG;AACH,WAAO;AACHQ,aAAO,EAAE,EADN;AAEHC,iBAAW,EAAE,IAFV;AAGHC,oBAAc,EAAEL,MAAM,CAACM,MAAP,CAAc,EAAd,EAAkB,KAAKP,SAAvB,CAHb;AAIHQ,iBAAW,EAAE,EAJV;AAKHC,iBAAW,EAAER,MAAM,CAACM,MAAP,CAAc,EAAd,EAAkB,KAAKJ,eAAvB;AALV,KAAP;AAQH,GAxCU;;AAyCXO,OAAK,EAAE;AACHV,aAAS,EAAE;AACPW,aAAO,EAAE,UAAUC,MAAV,EAAkB;AACvB,aAAKN,cAAL,GAAsBL,MAAM,CAACM,MAAP,CAAc,EAAd,EAAkBK,MAAlB,CAAtB;AACH,OAHM;AAIPC,UAAI,EAAE;AAJC;AADR,GAzCI;AAkDXC,UAAQ,EAAE;AACNC,sBAAkB,GAAG;AACjB,aAAO,CACH,QADG,EAEH,GAAG,KAAKvB,OAAL,CAAawB,GAAb,CAAkBC,MAAD,IAAYA,MAAM,CAACC,KAApC,CAFA,CAAP;AAIH,KANK;;AAONC,6BAAyB,GAAG;AACxB,aAAQ,iCAAgC,KAAKvB,IAAL,CAAUwB,MAAM,UAAxD;AACH,KATK;;AAUNC,oBAAgB,GAAG;AACf,aAAO,KAAKhB,WAAL,CAAiB,CAAjB,KAAuB,EAA9B;AACH,KAZK;;AAaN,OAAGhB,UAAU,CAAC;AACViC,cAAQ,EAAE,UADA;AAEVC,mBAAa,EAAE,eAFL;AAGVC,yBAAmB,EAAE,qBAHX;AAIVC,iBAAW,EAAE,aAJH;AAKVC,WAAK,EAAE;AALG,KAAD;AAbP,GAlDC;;AAuEXC,SAAO,GAAG;AACNC,WAAO,CAACC,GAAR,CAAY,cAAZ;AACA,SAAKC,WAAL;AACA,UAAMC,QAAO,GAAKzC,KAAK,CAAC0C,GAAN,CAAU,KAAKvB,WAAf,EAA4B,KAAKgB,WAAjC,CAAlB;AACA,UAAMQ,UAAS,GAAIF,QAAQ,CAACnC,IAA5B;AACA,SAAKY,WAAL,GAAmByB,UAAU,CAACjB,G