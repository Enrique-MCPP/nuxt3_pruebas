{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport { useTableBean } from '@/composables/useTableBean';\nimport PedidoService from '@/service/PedidoService';\nimport { computed } from 'vue';\nexport default {\n  setup() {\n    const pedidoService = new PedidoService();\n    const axiosConfig = computed(() => this.axiosConfig);\n    const tableBean = useTableBean(pedidoService, axiosConfig);\n    const save = editedRow => {\n      return tableBean.save(editedRow);\n    };\n    const doSomethingAfterSave = () => {\n      console.log('Se ha guardado la información');\n      console.log(tableBean.editedRow);\n    };\n    const saveWithLogic = () => {\n      // Cambiar el valor de editedRow aquí\n      tableBean.editedRow = {\n        ...tableBean.editedRow,\n        someProperty: 'someValue'\n      };\n      save(tableBean.editedRow).then(() => {\n        doSomethingAfterSave();\n      });\n    };\n    return {\n      editedRow: tableBean.editedRow,\n      selectedRow: tableBean.selectedRow,\n      list: tableBean.list,\n      isDisabled: tableBean.isDisabled,\n      save,\n      saveWithLogic\n    };\n  },\n  computed: {\n    ...mapGetters({\n      username: 'username',\n      keycloakToken: 'keycloakToken',\n      keycloakTokenParsed: 'keycloakTokenParsed',\n      axiosConfig: 'axiosConfig',\n      roles: 'roles'\n    })\n  }\n};","map":{"version":3,"names":["mapGetters","useTableBean","PedidoService","computed","setup","pedidoService","axiosConfig","tableBean","save","editedRow","doSomethingAfterSave","console","log","saveWithLogic","someProperty","then","selectedRow","list","isDisabled","username","keycloakToken","keycloakTokenParsed","roles"],"sources":["C:\\Users\\emalc\\OneDrive\\Escritorio\\CRUD_keycloak_primeVue\\keycloak-workshop\\frontend-vuejs\\src\\componentes\\PedidosCRUD.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <button @click=\"saveWithLogic\">Guardar</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nimport { useTableBean } from '@/composables/useTableBean';\r\nimport PedidoService from '@/service/PedidoService';\r\nimport { computed } from 'vue';\r\n\r\nexport default {\r\n  setup() {\r\n    const pedidoService = new PedidoService();\r\n    const axiosConfig = computed(() => this.axiosConfig);\r\n    const tableBean = useTableBean(pedidoService, axiosConfig);\r\n\r\n    const save = (editedRow) => {\r\n      return tableBean.save(editedRow);\r\n    };\r\n\r\n    const doSomethingAfterSave = () => {\r\n      console.log('Se ha guardado la información');\r\n      console.log(tableBean.editedRow);\r\n    };\r\n\r\n    const saveWithLogic = () => {\r\n      // Cambiar el valor de editedRow aquí\r\n      tableBean.editedRow = { ...tableBean.editedRow, someProperty: 'someValue' };\r\n\r\n      save(tableBean.editedRow).then(() => {\r\n        doSomethingAfterSave();\r\n      });\r\n    };\r\n\r\n    return {\r\n      editedRow: tableBean.editedRow,\r\n      selectedRow: tableBean.selectedRow,\r\n      list: tableBean.list,\r\n      isDisabled: tableBean.isDisabled,\r\n      save,\r\n      saveWithLogic,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters({\r\n      username: 'username',\r\n      keycloakToken: 'keycloakToken',\r\n      keycloakTokenParsed: 'keycloakTokenParsed',\r\n      axiosConfig: 'axiosConfig',\r\n      roles: 'roles',\r\n    }),\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAOA,SAASA,UAAS,QAAS,MAAM;AACjC,SAASC,YAAW,QAAS,4BAA4B;AACzD,OAAOC,aAAY,MAAO,yBAAyB;AACnD,SAASC,QAAO,QAAS,KAAK;AAE9B,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,aAAY,GAAI,IAAIH,aAAa,EAAE;IACzC,MAAMI,WAAU,GAAIH,QAAQ,CAAC,MAAM,IAAI,CAACG,WAAW,CAAC;IACpD,MAAMC,SAAQ,GAAIN,YAAY,CAACI,aAAa,EAAEC,WAAW,CAAC;IAE1D,MAAME,IAAG,GAAKC,SAAS,IAAK;MAC1B,OAAOF,SAAS,CAACC,IAAI,CAACC,SAAS,CAAC;IAClC,CAAC;IAED,MAAMC,oBAAmB,GAAIA,CAAA,KAAM;MACjCC,OAAO,CAACC,GAAG,CAAC,+BAA+B,CAAC;MAC5CD,OAAO,CAACC,GAAG,CAACL,SAAS,CAACE,SAAS,CAAC;IAClC,CAAC;IAED,MAAMI,aAAY,GAAIA,CAAA,KAAM;MAC1B;MACAN,SAAS,