ormación');\r\n      console.log(tableBean.editedRow);\r\n    };\r\n\r\n    const saveWithLogic = () => {\r\n      // Cambiar el valor de editedRow aquí\r\n      tableBean.editedRow = { ...tableBean.editedRow, someProperty: 'someValue' };\r\n\r\n      tableBean.save(tableBean.editedRow).then(() => {\r\n        doSomethingAfterSave();\r\n      });\r\n    };\r\n\r\n    return {\r\n      saveWithLogic,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters({\r\n      axiosConfig: 'axiosConfig',\r\n    }),\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAOA,SAASA,UAAS,QAAS,MAAM;AACjC,SAASC,YAAW,QAAS,4BAA4B;AACzD,OAAOC,aAAY,MAAO,yBAAyB;AACnD,SAASC,QAAO,QAAS,KAAK;AAE9B,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,aAAY,GAAI,IAAIH,aAAa,EAAE;IACzC,MAAMI,WAAU,GAAIH,QAAQ,CAAC,MAAM,IAAI,CAACG,WAAW,CAAC;IACpD,MAAMC,SAAQ,GAAIN,YAAY,CAACI,aAAa,EAAEC,WAAW,CAAC;IAE1D,MAAME,oBAAmB,GAAIA,CAAA,KAAM;MACjCC,OAAO,CAACC,GAAG,CAAC,+BAA+B,CAAC;MAC5CD,OAAO,CAACC,GAAG,CAACH,SAAS,CAACI,SAAS,CAAC;IAClC,CAAC;IAED,MAAMC,aAAY,GAAIA,CAAA,KAAM;MAC1B;MACAL,SAAS,CAACI,SAAQ,GAAI;QAAE,GAAGJ,SAAS,CAACI,SAAS;QAAEE,YAAY,EAAE;MAAY,CAAC;MAE3EN,SAAS,CAACO,IAAI,CAACP,SAAS,CAACI,SAAS,CAAC,CAACI,IAAI,CAAC,MAAM;QAC7CP,oBAAoB,EAAE;MACxB,CAAC,CAAC;IACJ,CAAC;IAED,OAAO;MACLI;IACF,CAAC;EACH,CAAC;EACDT,QAAQ,EAAE;IACR,GAAGH,UAAU,CAAC;MACZM,WAAW,EAAE;IACf,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module"}