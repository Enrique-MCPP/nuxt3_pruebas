{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport { useTableBean } from '@/composables//useTableBean';\nimport PedidoService from '@/service/PedidoService';\nimport { computed } from 'vue';\nexport default {\n  setup() {\n    const pedidoService = new PedidoService();\n    const axiosConfig = computed(() => this.axiosConfig);\n    const {\n      editedRow,\n      selectedRow,\n      list,\n      isDisabled,\n      save\n    } = useTableBean(pedidoService, axiosConfig);\n    editedRow = {\n      id: 1,\n      estado: \"aprobado\",\n      productos: [],\n      precioTotal: 15,\n      direccionEnvio: \"mi casa\",\n      metodoDePago: \"paypal\",\n      numSeguimiento: \"asdasd\",\n      fechaCreacion: new Date(),\n      observaciones: \"Primer editedRow al composable\"\n    };\n    const doSomethingAfterSave = () => {\n      console.log('Se ha guardado la información');\n    };\n    const saveWithLogic = () => {\n      if (editedRow) {\n        save(editedRow).then(() => {\n          doSomethingAfterSave();\n        });\n      }\n    };\n    return {\n      editedRow,\n      selectedRow,\n      list,\n      isDisabled,\n      save,\n      saveWithLogic\n    };\n  },\n  computed: {\n    ...mapGetters({\n      username: 'username',\n      keycloakToken: 'keycloakToken',\n      keycloakTokenParsed: 'keycloakTokenParsed',\n      axiosConfig: 'axiosConfig',\n      roles: 'roles'\n    })\n  }\n};","map":{"version":3,"names":["mapGetters","useTableBean","PedidoService","computed","setup","pedidoService","axiosConfig","editedRow","selectedRow","list","isDisabled","save","id","estado","productos","precioTotal","direccionEnvio","metodoDePago","numSeguimiento","fechaCreacion","Date","observaciones","doSomethingAfterSave","console","log","saveWithLogic","then","username","keycloakToken","keycloakTokenParsed","roles"],"sources":["C:\\Users\\emalc\\OneDrive\\Escritorio\\CRUD_keycloak_primeVue\\keycloak-workshop\\frontend-vuejs\\src\\componentes\\PedidosCRUD.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <button @click=\"saveWithLogic\">Guardar</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nimport { useTableBean } from '@/composables//useTableBean';\r\nimport PedidoService from '@/service/PedidoService';\r\nimport { computed } from 'vue';\r\n\r\nexport default {\r\n  setup() {\r\n    const pedidoService = new PedidoService();\r\n    const axiosConfig = computed(() => this.axiosConfig);\r\n    const {editedRow, selectedRow, list, isDisabled, save } = useTableBean(pedidoService, axiosConfig);\r\n\r\n    editedRow = {\r\n      id: 1,\r\n      estado: \"aprobado\",\r\n      productos: [],\r\n      precioTotal: 15,\r\n      direccionEnvio: \"mi casa\",\r\n      metodoDePago: \"paypal\",\r\n      numSeguimiento: \"asdasd\",\r\n      fechaCreacion: new Date(),\r\n      observaciones: \"Primer editedRow al composable\",\r\n    };\r\n\r\n    const doSomethingAfterSave = () => {\r\n      console.log('Se ha guardado la información');\r\n    }\r\n\r\n    const saveWithLogic = () => {\r\n      if (editedRow) {\r\n        save(editedRow).then(() => {\r\n          doSomethingAfterSave();\r\n        });\r\n      }\r\n    }\r\n\r\n    return {\r\n      editedRow,\r\n      selectedRow,\r\n      list,\r\n      isDisabled,\r\n      save,\r\n      saveWithLogic,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters({\r\n      username: 'username',\r\n      keycloakToken: 'keycloakToken',\r\n      keycloakTokenParsed: 'keycloakTokenParsed',\r\n      axiosConfig: 'axiosConfig',\r\n      roles: 'roles',\r\n    }),\r\n  },\r\n};\r\n\r\n</script>\r\n"],"mappings":"AAOA,SAASA,UAAS,QAAS,MAAM;AACjC,SAASC,YAAW,QAAS,6BAA6B;AAC1D,OAAOC,aAAY,MAAO,yBAAyB;AACnD,SAASC,QAAO,QAAS,KAAK;AAE9B,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,aAAY,GAAI,IAAIH,aAAa,EAAE;IACzC,MAAMI,WAAU,GAAIH,QAAQ,CAAC,MAAM,IAAI,CAACG,WAAW,CAAC;IACpD,MAAM;MAACC,SAAS;MAAEC,WAAW;MAAEC,IAAI;MAAEC,UAAU;MAAEC;IAAK,IAAIV,YAAY,CAACI,aAAa,EAAEC,WAAW,CAAC;I